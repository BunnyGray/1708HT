<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tarena.ht.mapper.PublishMapper">
    <select id="findAll" resultType="string">
    SELECT m.msg FROM message m
    </select>
    <insert id="addPublish">
    INSERT INTO message SET msg=#{msg}

    </insert>
</mapper>